{
  "name": "ws-roomified-client",
  "version": "0.5.291",
  "description": "Client implementation of the ws-roomified module",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisitopherus/ws-roomified-client.git"
  },
  "keywords": [
    "ws",
    "client",
    "wrapper",
    "ts"
  ],
  "author": "chrisitopherus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisitopherus/ws-roomified-client/issues"
  },
  "homepage": "https://chrisitopherus.github.io/ws-roomified-client",
  "devDependencies": {
    "typescript": "^4.7.4"
  }
}